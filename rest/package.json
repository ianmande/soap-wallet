{
  "name": "wallet-rest-service",
  "version": "1.0.0",
  "description": "Servicio REST para ePayco",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "dev:nodemon": "nodemon --exec tsx src/app.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:e2e": "jest tests/e2e"
  },
  "keywords": [
    "wallet",
    "rest",
    "express",
    "epayco",
    "nodejs",
    "typescript"
  ],
  "author": "ePayco Developer",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  }
}
